        "# convert to datetime\n",
        "uberData['Date/Time'] = uberData['Date/Time'].map(pd.to_datetime)"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab_type": "code",
        "id": "mioKDXruWFZ7",
        "colab": {}
      },
      "source": [
        "# functions to extract the day of the month, weekday and hour\n",
        "def get_dom(dt):\n",
        "    return dt.day\n",
        "uberData['Day of Month'] = uberData['Date/Time'].map(get_dom)\n",
        "\n",
        "def get_weekday(dt):\n",
        "    return dt.weekday()\n",
        "uberData['Weekday'] = uberData['Date/Time'].map(get_weekday)\n",
        "\n",
        "def get_hour(dt):\n",
        "    return dt.hour\n",
        "uberData['Hour'] = uberData['Date/Time'].map(get_hour)"
      ],
      "execution_count": 0,
      "outputs": []
